  "defaultSortField": "name",
  "endpoint": "/layers",
  "entityType": "layer",
  "entityName": "#i18n.entityName",
  "navigationTitle": "#i18n.navigationTitle",
  "subTitle": "",
  "defaultEntity": {
    "type": "TILEWMS",
    "clientConfig": {
      "hoverable": false
    },
    "sourceConfig": {
      "url": "https://ows.terrestris.de/osm/service?",
      "layerNames": "OSM-WMS",
      "useBearerToken": false
    }
  },
  "formConfig": {
    "name": "layer",
    "fields": [
      {
        "dataType": "number",
        "dataField": "id",
        "label": "#i18n.labelId",
        "readOnly": "true"
      },
      {
        "dataField": "created",
        "dataType": "date",
        "readOnly": "true",
        "component": "DateField",
        "label": "#i18n.labelEstabl",
        "fieldProps": {
          "dateFormat": "DD.MM.YYYY HH:mm"
        }
      },
      {
        "dataField": "modified",
        "dataType": "date",
        "readOnly": "true",
        "label": "#i18n.labelEdit",
        "component": "DateField",
        "fieldProps": {
          "dateFormat": "DD.MM.YYYY HH:mm"
        }
      },
      {
        "component": "Input",
        "dataField": "name",
        "label": "#i18n.labelName",
        "required": "true"
      },
      {
        "component": "LayerTypeSelect",
        "dataField": "type",
        "label": "#i18n.labelType",
        "required": "true",
        "fieldProps": {
          "supportedTypes": [
            "TILEWMS",
            "WFS",
            "WMS",
            "WMSTIME",
            "WMTS",
            "XYZ"
          ]
        }
      },
      {
        "component": "JSONEditor",
        "dataField": "clientConfig",
        "label": "#i18n.labelConfig",
        "fieldProps": {}
      },
      {
        "component": "JSONEditor",
        "dataField": "sourceConfig",
        "label": "#i18n.labelSource",
        "fieldProps": {}
      }
    ]
  },
  "tableConfig": {
    "columnDefinition": [
      {
        "title": "#i18n.titleId",
        "dataIndex": "id",
        "sortConfig": {
          "isSortable": true
        }
      },
      {
        "title": "#i18n.titleName",
        "dataIndex": "name",
        "key": "name",
        "sortConfig": {
          "isSortable": true
        },
        "filterConfig": {
          "isFilterable": true
        }
      },
      {
        "title": "#i18n.titleType",
        "dataIndex": "type",
        "key": "type",
        "sortConfig": {
          "isSortable": true
        }
      },
      {
        "title": "#i18n.titlePrev",
        "key": "preview",
        "cellRenderComponentName": "LayerPreviewCell"
      }
    ]
  }
}
